{
    "name": "subodh/smart-ai-assistant",
    "description": "A reusable Laravel package providing a smart AI assistant for AEPS error handling.",
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "Subodh",
            "email": "subodh@example.com"
        }
    ],
    "require": {
        "php": ">=8.1",
        "illuminate/support": "^10.0|^11.0",
        "phpoffice/phpspreadsheet": "^1.29"
    },
    "autoload": {
        "psr-4": {
            "Subodh\\SmartAiAssistant\\": "src/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Subodh\\SmartAiAssistant\\SmartAiAssistantServiceProvider"
            ]
        }
    },
    "minimum-stability": "stable",
    "prefer-stable": true
}